{
  "name": "camp-backend",
  "version": "1.0.0",
  "description": "Mock backend for Campus Asset Management Platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "mocha 'tests/**/*.test.js' --timeout=5000 --exit",
    "test:coverage": "node ./node_modules/c8/bin/c8.js --check-coverage --branches 10 --functions 10 --lines 10 --statements 10 mocha 'tests/**/*.test.js' --timeout=5000 --exit",
    "test:mocha": "node ./node_modules/mocha/bin/mocha.js 'tests/**/*.test.js' --timeout=5000"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^9.0.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "c8": "^9.1.0",
    "chai": "^4.3.10",
    "supertest": "^7.0.0",
    "mocha": "^11.0.0",
    "nodemon": "^3.1.0"
  }
}
